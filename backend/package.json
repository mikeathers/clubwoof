{
  "name": "backend",
  "version": "0.1.0",
  "bin": {
    "backend": "bin/backend.js"
  },
  "scripts": {
    "setup": "npm install && npm install --prefix src",
    "deploy-all-prod": "npx cdk deploy *-prod --outputs-file ../src/cdk-exports-prod.json",
    "deploy-frontend-prod": "npx cdk deploy clubwoof-website-prod --outputs-file ../src/cdk-exports-prod.json",
    "deploy-backend-prod": "npx cdk deploy clubwoof-backend-prod --outputs-file ../src/cdk-exports-prod.json",
    "deploy-all-dev": "npx cdk deploy *-dev --outputs-file ../src/cdk-exports-dev.json",
    "deploy-frontend-dev": "npx cdk deploy clubwoof-website-dev --outputs-file ../src/cdk-exports-dev.json",
    "deploy-backend-dev": "npx cdk deploy clubwoof-backend-dev --outputs-file ../src/cdk-exports-dev.json",
    "test": "jest",
    "watch": "tsc -w",
    "build": "tsc",
    "lint": "eslint . --fix --ext ts,tsx"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.109",
    "@types/jest": "^29.2.3",
    "@types/node": "18.11.9",
    "@types/prettier": "2.6.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "aws-cdk": "^2.50.0",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1274.0",
    "esbuild": "^0.15.12",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "prettier": "^2.3.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "aws-cdk-lib": "2.54.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
