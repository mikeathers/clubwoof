{
  "name": "clubwoof",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export:dev": "next export -o backend/frontend-build/dev",
    "export:prod": "next export -o backend/frontend-build/prod",
    "build:dev": "env-cmd -f ./env/.env.development next build && npm run export:dev",
    "build:prod": "env-cmd -f ./env/.env.production next build && npm run export:prod",
    "start": "env-cmd -f src/env/.env.development next start",
    "lint": "eslint . --fix --ext ts,tsx",
    "prepare": "husky install",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "prettier:fix": "prettier -w src/* backend/src/*",
    "test": "jest --watch",
    "storybook:start": "start-storybook -p 6006 -s ./public",
    "storybook:build": "build-storybook -s ./public -o backend/storybook-build"
  },
  "dependencies": {
    "@aws-amplify/auth": "^3.4.29",
    "@hookform/resolvers": "^2.9.10",
    "@next/font": "13.0.7",
    "dotenv": "^16.0.3",
    "env-cmd": "^10.1.0",
    "grommet": "^2.28.0",
    "next": "13.0.7",
    "next-export-i18n": "^2.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.41.3",
    "react-icons": "^4.7.1",
    "react-responsive": "^9.0.2",
    "styled-components": "^5.3.6",
    "webpack": "^5.75.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@jest/globals": "^29.3.1",
    "@next/bundle-analyzer": "^10.0.8",
    "@next/eslint-plugin-next": "^13.1.1",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/dom": "^8.19.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.5",
    "@types/node": "18.11.17",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@types/styled-components": "^5.1.26",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "babel-jest": "^28.1.3",
    "babel-loader": "^8.3.0",
    "eslint": "^7.6.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-storybook": "^0.6.10",
    "husky": "^8.0.0",
    "i18next": "^22.4.9",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^29.3.1",
    "jest-mock": "^29.3.1",
    "next-router-mock": "^0.9.1-beta.0",
    "prettier": "^2.3.2",
    "storybook-addon-mock": "^3.2.0",
    "storybook-addon-next-router": "^4.0.2",
    "ts-jest": "^28.0.8",
    "typescript": "^4.9.4",
    "typescript-eslint": "^0.0.1-alpha.0"
  }
}
